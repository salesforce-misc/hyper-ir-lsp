{
  "comments": {
    "lineComment": "#",
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "wordPattern": "(-?\\d*\\.\\d\\w*)|(-?0x\\d*)|([a-zA-Z0-9_@%!][a-zA-Z0-9_:]+[a-zA-Z0-9_])"
}
